{
  "main": "node_modules/expo/AppEntry.js",
  "lint-staged": {
    "*.{tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "start": "expo start && echo \"\n\nWelcome to Gladeo! \n\nStarting Mobile app....\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watch --coverage=false --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint src/**/**/*.tsx",
    "lint-fix": "eslint --fix src/**/**/*.tsx",
    "precommit": "lint-staged && yarn lint",
    "eslint-check": "eslint --print-config src/**/*/*.ts | eslint-config-prettier-check",
    "server": "node server/server.js --exec nodemon",
    "dev": "run-p server start"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^10.0.3",
    "@react-navigation/core": "^3.0.0-alpha.13",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.9",
    "@types/react-native": "^0.60.21",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^16.9.1",
    "airtable": "^0.7.2",
    "core-js": "3",
    "dotenv": "^8.2.0",
    "expo": "^35.0.0",
    "expo-asset": "^7.0.0",
    "expo-constants": "^7.0.0",
    "expo-font": "^7.0.0",
    "expo-web-browser": "^7.0.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-video": "^5.0.2",
    "react-native-web": "^0.11.7",
    "react-navigation": "3",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-preset-expo": "^7.0.0",
    "babel-preset-react-native": "^4.0.1",
    "body-parser": "^1.19.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "express": "^4.17.1",
    "jest-expo": "^35.0.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.8.3"
  },
  "private": true,
  "proxy": "http://localhost:3001"
}
